{
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
              "phpunit",
              "Tests", "Test", "tests", "test"
            ],
            "in": "vendor"
        },
        {
            "name": "*.png",
            "in": "images"
        },
        {
            "name": "*.csv",
            "in": "vendor/totten/license-data"
        },
        {
            "name": "*.txt",
            "in": "vendor/totten/license-data/licenses"
        }
    ],
    "git-version": "package_version",
    "main": "bin/civix",
    "compactors": ["KevinGH\\Box\\Compactor\\PhpScoper"],
    "output": "bin/civix.phar",
    "stub": true
}
